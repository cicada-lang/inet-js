{
  "name": "@cicada-lang/inet",
  "version": "0.1.35",
  "repository": "github:cicada-lang/inet",
  "main": "./lib/index.js",
  "files": [
    "lib"
  ],
  "bin": {
    "inet.js": "bin/inet.js"
  },
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "test:ts": "vitest --dir src --threads false --run",
    "test:inet": "test-runner snapshot 'node ./bin/inet.js run --no-color' 'std/**/*.i' --exclude 'std/**/*.error.i'",
    "test:inet-error": "test-runner snapshot-error 'node ./bin/inet.js run --no-color' 'std/**/*.error.i'",
    "test:inet-parse": "test-runner test 'node ./bin/inet.js parse --no-color' 'std/**/*.i'",
    "test:inet-format": "test-runner test 'node ./bin/inet.js format --no-color' 'std/**/*.i'",
    "test:todo": "npm run test:inet && npm run test:inet-error",
    "test": "npm run test:inet-parse && npm run test:inet-format",
    "format": "prettier src docs --write"
  },
  "devDependencies": {
    "@types/node": "^20.6.2",
    "@xieyuheng/test-runner": "^0.2.9",
    "prettier": "^3.0.3",
    "prettier-plugin-organize-imports": "^3.2.3",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@cicada-lang/framework": "^0.1.5",
    "@cicada-lang/partech": "^0.2.5",
    "@xieyuheng/command-line": "^0.0.11",
    "@xieyuheng/ty": "^0.1.23",
    "picocolors": "^1.0.0",
    "vitest": "^0.34.4"
  },
  "license": "GPL-3.0-or-later"
}
